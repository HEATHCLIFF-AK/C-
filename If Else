#include <iostream>
using namespace std;

int main() {
   int a, b, c;
   cout<<"Drop 3 numbers: "<<endl;
   cin>>a>>b>>c;
   cout<<"The Maximum Number is : ";
   
   if(a>b){
       if(a>c){
           cout<<a<<endl;
       }
       else{
           cout<<c<<endl;
       }
   }
   else{
       if(b>c){
           cout<<b<<endl;
       }
       else{
           cout<<c<<endl;
       }
   }

    return 0;
}
